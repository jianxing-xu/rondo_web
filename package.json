  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --open --host",
    "test": "yarn serve:test 'cypress open'",
    "test:headless": "yarn serve:test 'cypress run'",
    "build": "tsc && vite build",
    "serve": "vite preview",
