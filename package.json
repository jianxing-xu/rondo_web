    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "34.0.1",
    "husky": "7.0.1",
    "less": "^4.1.1",
    "lint-staged": "11.1.1",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "qs": "^6.5.2",
    "rollup-plugin-istanbul": "3.0.0",
    "start-server-and-test": "1.13.1",
    "stylelint": "13.13.1",
